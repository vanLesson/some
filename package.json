{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn update-config && react-app-rewired start",
    "build": "react-app-rewired build",
    "compile": "yarn update-config && tsc -p ./tsconfig.json --noEmit",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{ts,tsx}'",
    "build:production": "env-cmd -f .env env-cmd -f .env.production react-app-rewired build",
    "build:staging": "env-cmd -f .env env-cmd -f .env.staging react-app-rewired build",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:push": "sh scripts/push-docker.sh",
    "graphql-generate": "graphql-codegen",
    "update-config": "ts-node --project scripts/tsconfig.json scripts/update-config.ts",
    "update-localization": "node scripts/update-localization.js"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.6.1",
    "@emotion/core": "^10.0.28",
    "@material-ui/core": "4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.52",
    "@material-ui/pickers": "^3.2.10",
    "@react-native-community/async-storage-backend-web": "^2.0.0-rc.3",
    "@react-yuki/ui": "^0.0.38",
    "@spryrocks/config-react": "^1.0.0-alpha.7",
    "@spryrocks/react-api": "^0.1.4",
    "@spryrocks/react-auth": "^0.1.2-dev1",
    "@types/jquery": "^3.5.0",
    "@types/react-slick": "^0.23.4",
    "apollo-boost": "^0.4.7",
    "date-fns": "^2.14.0",
    "firebase": "7.14.2",
    "formik": "^2.1.4",
    "history": "4.10.1",
    "i18next": "^17.0.18",
    "i18next-browser-languagedetector": "^4.0.2",
    "jquery": "^3.5.1",
    "material-ui-chip-input": "^1.1.0",
    "mediasoup-client": "^3.6.5",
    "node-sass": "4.14.1",
    "promise-queue": "2.2.5",
    "query-string": "^6.12.1",
    "ramda": "0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "11.4.0",
    "react-id-swiper": "^4.0.0",
    "react-redux": "7.2.0",
    "react-relay": "^9.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-video": "^1.5.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "2.13.8",
    "redux-form": "8.3.5",
    "redux-saga": "1.1.3",
    "relay-compiler-language-typescript": "^12.0.3",
    "rxjs": "^6.5.5",
    "slick-carousel": "^1.8.1",
    "swiper": "^5.4.5",
    "uuid": "^8.1.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.10.0",
    "@graphql-codegen/typescript": "^1.10.0",
    "@spryrocks/eslint-config-react": "^0.1.0",
    "@types/promise-queue": "^2.2.0",
    "@types/ramda": "^0.27.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/swiper": "^5.4.0",
    "@types/uuid": "^8.0.0",
    "@types/yup": "^0.29.1",
    "babel-plugin-relay": "^9.1.0",
    "env-cmd": "^10.1.0",
    "husky": "^4.2.5",
    "react-app-rewired": "^2.1.5",
    "relay-compiler": "^9.1.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn lint"
    }
  }
}
