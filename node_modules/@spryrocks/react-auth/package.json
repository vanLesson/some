{
  "name": "@spryrocks/react-auth",
  "version": "0.1.4",
  "main": "index.js",
  "repository": "https://gitlab.com/spry-rocks/modules/spry-rocks-react-auth.git",
  "author": "maximzhemerenko <maxim.zhemerenko@spryrocks.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p ./tsconfig.json --noEmit",
    "lint": "eslint './**/*.{ts,tsx}'",
    "build": "tsc",
    "yarn:prepare": "rm -rf lib && mkdir lib && yarn build && cp package.json lib",
    "yarn:publish": "yarn yarn:prepare && yarn publish lib --access public"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^2.0.0-rc.3"
  },
  "devDependencies": {
    "@spryrocks/eslint-config": "^0.1.1",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn lint"
    }
  }
}
