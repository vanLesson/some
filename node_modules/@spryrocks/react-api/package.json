{
  "name": "@spryrocks/react-api",
  "version": "0.1.4",
  "repository": "https://gitlab.com/spry-rocks/modules/spry-rocks-modules-react-api.git",
  "author": "maximzhemerenko <maxim.zhemerenko@spryrocks.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p ./tsconfig.json --noEmit",
    "lint": "eslint './**/*.{ts,tsx}'",
    "build": "tsc",
    "yarn:publish": "yarn build && cp package.json lib && yarn publish lib --access public"
  },
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.20",
    "axios": "^0.19.2",
    "axios-logger": "^2.3.1",
    "graphql": "^14.6.0",
    "promise-queue": "^2.2.5"
  },
  "devDependencies": {
    "@spryrocks/eslint-config": "^0.1.1",
    "@types/promise-queue": "^2.2.0",
    "typescript": "^3.8.3",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn lint"
    }
  }
}
